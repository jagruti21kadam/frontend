// Q8: Normal function to multiply two numbers
function multiplyNumbers(a: number, b: number): number {
    return a * b;
}

// Converting the above function to an arrow function
const multiplyNumbersArrow = (a: number, b: number): number => a * b;

// Q9: Arrow function to sum an array of numbers
const sum = (numbers: number[]): number => numbers.reduce((acc, curr) => acc + curr, 0);

// Example usage:
console.log(multiplyNumbers(4, 5)); // Output: 20
console.log(multiplyNumbersArrow(4, 5)); // Output: 20
console.log(sum([1, 2, 3, 4, 5])); // Output: 15
