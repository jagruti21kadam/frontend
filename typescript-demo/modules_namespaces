// Module: mathOperations
export function add(a: number, b: number): number {
    return a + b;
}

// Namespace: Geometry
namespace Geometry {
    export function calculateArea(radius: number): number {
        return Math.PI * radius * radius;
    }
}

// Using the module function
const num1 = 5;
const num2 = 10;
console.log(`Sum: ${add(num1, num2)}`);

// Using the namespace function
const radius = 7;
console.log(`Area of Circle: ${Geometry.calculateArea(radius)}`);
